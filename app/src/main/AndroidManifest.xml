<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sastrenic">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_index"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SASTRENIC">
        <activity
            android:name=".NuevoClienteActivity"
            android:parentActivityName=".ClienteActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ClienteActivity" />
        </activity>
        <activity
            android:name=".ClienteActivity"
            android:parentActivityName=".InicioActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".InicioActivity" />
        </activity>
        <activity
            android:name=".InicioActivity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>